{
  "name": "rescript-project-template",
  "version": "0.0.1",
  "sources": [
    {
      "dir" : "src",
      "subdirs" : true
    },
    {
      "dir" : "test",
      "subdirs" : true,
      "type": "dev"
    }
  ], 
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".js",
  "bs-dependencies": [
    "@ryyppy/rescript-promise",
    "rescript-test",
    "rescript-jzon",
    "rescript-uuid"
  ],
  "external-stdlib" : "@rescript/std",
  "warnings": {
    "error" : "+101"
  }
}
